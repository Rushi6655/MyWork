<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
	<servlet>
		<servlet-name>adminlogin</servlet-name>
		<servlet-class>Servlets.AdminLogin</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>adminlogin</servlet-name>
		<url-pattern>/login</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>addStudent</servlet-name>
		<servlet-class>Servlets.addStudent</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>addStudent</servlet-name>
		<url-pattern>/addstudent</url-pattern>
	</servlet-mapping>
	<servlet>
	
	
		<servlet-name>feedback</servlet-name>
		<servlet-class>Servlets.FeedbackPage</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>feedback</servlet-name>
		<url-pattern>/StudentFeedBack</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>studentlogin</servlet-name>
		<servlet-class>Servlets.studentlogin</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>studentlogin</servlet-name>
		<url-pattern>/slogin</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>feedbacktable</servlet-name>
		<servlet-class>Servlets.getAllFeedbacks</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>feedbacktable</servlet-name>
		<url-pattern>/getAll</url-pattern>
	</servlet-mapping>
</web-app>
